# Fichier d'exemple pour snmp_exporter
# REMPLACER 'votre_communaute' PAR VOTRE VALEUR REELLE (HORS GITHUB)

auths:
  default:
    community: <VOTRE_COMMUNAUTE_SNMP>

modules:
  mikrotik:
    walk:
      - 1.3.6.1.2.1.1.3               # sysUpTime
      - 1.3.6.1.2.1.1.5               # sysName (Identit√©)
      - 1.3.6.1.2.1.25.3.3.1.2        # cpuLoad
      - 1.3.6.1.2.1.25.2.3.1.5        # hrStorageSize
      - 1.3.6.1.2.1.25.2.3.1.6        # hrStorageUsed
      - 1.3.6.1.4.1.14988.1.1.4.4     # ROS Version
      - 1.3.6.1.2.1.31.1.1.1.6        # ifHCInOctets (Traffic Entrant 64-bit)
      - 1.3.6.1.2.1.31.1.1.1.10       # ifHCOutOctets (Traffic Sortant 64-bit)
    metrics:
      - name: sysUpTime
        oid: 1.3.6.1.2.1.1.3.0
        type: gauge
      - name: sysName
        oid: 1.3.6.1.2.1.1.5.0
        type: DisplayString
      - name: ifInOctets
        oid: 1.3.6.1.2.1.31.1.1.1.6
        type: counter
      - name: ifOutOctets
        oid: 1.3.6.1.2.1.31.1.1.1.10
        type: counter